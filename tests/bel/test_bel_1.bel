# Statements Section

SET STATEMENT_GROUP = "Group 1"

SET TESTAN1 = "TestAnnot1"
SET Citation = {"Pubmed","That one article from last week","123455"}

SET Evidence = "Evidence 1 /
w extra notes"

p(TESTNS1:TestValue1) -> p(TESTNS1:TestValue2)

SET Evidence = "Evidence 2"
