SET Evidence = "Evidence 3"

p(TESTNS1:TestValue4) -- p(TESTNS1:TestValue5)
p(TESTNS1:TestValue1) -- ((p(TESTNS1:TestValue2) -- p(TESTNS1:TestValue3))
